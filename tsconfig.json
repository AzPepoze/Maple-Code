{
	"compilerOptions": {
		"module": "es2020",
		"moduleResolution": "node",
		"lib": ["ES2022", "dom"],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": false,
		"noImplicitAny": false,
		"strictNullChecks": false
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules", "dist"]
}
